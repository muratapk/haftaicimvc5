@{
    if (TempData["Success"] != null)
    {
        <div class="alert alert-warning" role="alert">
            @TempData["Success"]
        </div>
    }
    if (TempData["Error"] != null)
    {
        <div class="alert alert-danger" role="alert">
            @TempData["Error"]
        </div>
    }

}
